  "Eratosthenes Sieve": {
    "prefix": "eratosthenes_sieve",
    "body": [
      "std::vector<int64_t> eratosthenes_sieve(int64_t N) {",
      "  std::vector<int64_t> primes;",
      "  std::vector<bool> is_prime(N + 1, true);",
      "  is_prime.at(0) = false;",
      "  is_prime.at(1) = false;",
      "",
      "  for (int64_t i = 2; i <= N; i++) {",
      "    if (is_prime.at(i)) {",
      "      primes.push_back(i);",
      "      for (int64_t j = 2 * i; j <= N; j += i) {",
      "        is_prime.at(j) = false;",
      "      }",
      "    }",
      "  }",
      "",
      "  return primes;",
      "}"
    ],
    "description": "Implementation of Eratosthenese Sieve"
  },
  "Iterative Power": {
    "prefix": "iterative_power",
    "body": [
      "int64_t iterative_power(int64_t a, int64_t n, int64_t p) {",
      "  if (n == 0) return 1;",
      "  if (n == 1) return a % p;",
      "  if (n % 2 == 1) return (a * iterative_power(a, n - 1, p)) % p;",
      "",
      "  int64_t t = iterative_power(a, n / 2, p);",
      "  return (t * t) %p;",
      "}"
    ],
    "description": "Implementation of Iterative Power"
  },
